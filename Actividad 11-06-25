import java.util.Scanner;
//[ ]
public class Actividad {
//En la parte mas alta se decclaran los argumentos y variables
    public static void main(String[] args) {
        Scanner teclado = new Scanner(System.in);
        float calificacion; //Calificacion de un estudiante

        //Preguntar al usuario por su calificacion
        System.out.println("Ingrese la calificacion obtenida del 0 al 10");
        //Capturamos el valor que el usuario ingresa
        calificacion = teclado.nextFloat();

        if (calificacion <= 5){
            System.out.println("Reprobado");
        } else {
            System.out.println("Aprobado, sigue asi"); 
        }
    }
}
